{
  "name": "PayPalRetailSDK",
  "version": "0.5.0",
  "summary": "Retail SDK",
  "description": "Retail SDK",
  "homepage": "https://github.com/PayPal-Mobile/paypal-retail-sdk",
  "license": "MS-RSL",
  "authors": {
    "PayPal": "DL-PP-RetailSDK@paypal.com"
  },
  "source": {
    "git": "git@github.paypal.com:RetailSDK-NewGen/retail-sdk.git",
    "branch": "develop"
  },
  "platforms": {
    "ios": "7.0",
    "osx": "10.10"
  },
  "prefix_header_contents": "#import \"PayPalRetailSDK+Private.h\"",
  "requires_arc": true,
  "ios": {
    "vendored_frameworks": [
      "iOS/Roam/RUA_BLE.framework",
      "iOS/Roam/LandiSDK_BLE.framework",
      "iOS/Roam/G4XSwiper.framework"
    ]
  },
  "default_subspecs": "Default",
  "subspecs": [
    {
      "name": "IncludeRoam",
      "exclude_files": [
        "Common/generated/PayPalRetailTest/**/*",
        "Common/Tests/**/*"
      ],
      "resource_bundles": {
        "PayPalRetailSDKResources": [
          "Common/AllPlatforms/PayPalRetailSDK.js",
          "Common/AllPlatforms/sounds/*",
          "iOS/PayPalRetailSDK/assets/*",
          "iOS/PayPalRetailSDK/*.png"
        ]
      },
      "public_header_files": [
        "Common/generated/*.h",
        "Common/PPRetailObject.h",
        "Common/PayPalRetailSDK.h",
        "Common/SdkCredential.h",
        "pph/*.h"
      ],
      "ios": {
        "frameworks": [
          "Foundation",
          "UIKit",
          "ExternalAccessory",
          "JavaScriptCore",
          "MediaPlayer"
        ],
        "source_files": [
          "iOS/PayPalRetailSDK/PayPalRetailSDK/**/*.{h,m,mm}",
          "iOS/Roam/*.{h,m,mm}",
          "Common/*.{h,m,mm}",
          "Common/generated/**/*.{h,m,mm}",
          "Common/manticore-objc/Manticore/*.{h,m,mm}",
          "pph/**/*.{h,m,mm}"
        ]
      },
      "osx": {
        "frameworks": [
          "Foundation",
          "AppKit"
        ],
        "dependencies": {
          "ORSSerialPort": [
            "~> 1.7.1"
          ]
        },
        "source_files": [
          "osx/PayPalRetailSDK/PayPalRetailSDK/**/*.{h,m,mm}",
          "Common/*.{h,m,mm}",
          "Common/generated/**/*.{h,m,mm}",
          "Common/manticore-objc/Manticore/*.{h,m,mm}",
          "pph/**/*.{h,m,mm}"
        ]
      },
      "dependencies": {
        "SimpleKeychain": [
          "~> 0.6.1"
        ]
      },
      "xcconfig": {
        "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) INCLUDE_ROAM_AUDIO"
      },
      "libraries": [
        "c++",
        "stdc++",
        "z"
      ]
    },
    {
      "name": "Default",
      "exclude_files": [
        "Common/generated/PayPalRetailTest/**/*",
        "Common/Tests/**/*"
      ],
      "resource_bundles": {
        "PayPalRetailSDKResources": [
          "Common/AllPlatforms/PayPalRetailSDK.js",
          "Common/AllPlatforms/sounds/*",
          "iOS/PayPalRetailSDK/assets/*",
          "iOS/PayPalRetailSDK/*.png"
        ]
      },
      "public_header_files": [
        "Common/generated/*.h",
        "Common/PPRetailObject.h",
        "Common/PayPalRetailSDK.h",
        "Common/SdkCredential.h",
        "pph/*.h"
      ],
      "ios": {
        "frameworks": [
          "Foundation",
          "UIKit",
          "ExternalAccessory",
          "JavaScriptCore",
          "MediaPlayer"
        ],
        "source_files": [
          "iOS/PayPalRetailSDK/PayPalRetailSDK/**/*.{h,m,mm}",
          "iOS/Roam/*.{h,m,mm}",
          "Common/*.{h,m,mm}",
          "Common/generated/**/*.{h,m,mm}",
          "Common/manticore-objc/Manticore/*.{h,m,mm}",
          "pph/**/*.{h,m,mm}"
        ]
      },
      "osx": {
        "frameworks": [
          "Foundation",
          "AppKit"
        ],
        "dependencies": {
          "ORSSerialPort": [
            "~> 1.7.1"
          ]
        },
        "source_files": [
          "osx/PayPalRetailSDK/PayPalRetailSDK/**/*.{h,m,mm}",
          "Common/*.{h,m,mm}",
          "Common/generated/**/*.{h,m,mm}",
          "Common/manticore-objc/Manticore/*.{h,m,mm}",
          "pph/**/*.{h,m,mm}"
        ]
      },
      "dependencies": {
        "SimpleKeychain": [
          "~> 0.6.1"
        ]
      },
      "xcconfig": {
        "OTHER_LDFLAGS": "-weak_library /usr/lib/libstdc++.dylib -lstdc++ -ObjC"
      },
      "libraries": [
        "c++",
        "stdc++",
        "z"
      ]
    }
  ]
}
